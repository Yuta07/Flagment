<div id ="login-form">
<%= form_for(:session, url: login_path) do |f| %>
  <!-- email -->
  <%= f.label :Email %>
  <%= f.email_field :email %>
  <!-- password -->
  <%= f.label :Password %>
  <%= f.password_field :password %>
  <!-- checkbox -->
  <%= f.label :Remember_me do %>
    <%= f.check_box :remember_me %>
    <span>Remember me on this computer</span>
  <% end %>
  <%= link_to("Forget password?",new_password_reset_path) %>
  <!-- submit -->
  <%= f.submit "Log in" %>
<% end %>
</div>
